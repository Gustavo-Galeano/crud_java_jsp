	<queryString>
		<![CDATA[SELECT

     ventas.id_venta AS ven_idventa,
     clientes.id_cliente AS ventas_Clientes_id,
     ventas.fecha AS ventas_ven_fecha,
     clientes.apellido AS clientes_cli_apellido,
     clientes.nombre AS clientes_cli_nombre,
     clientes.ruc AS clientes_cli_ruc,
     clientes.telefono AS clientes_cli_telefono,
CONCAT(clientes.nombre,' ', clientes.apellido) AS clientes

FROM
     clientes clientes INNER JOIN ventas ventas ON clientes.id_cliente = ventas.id_cliente
where ventas.id_venta=$P{FacturaN}]]>
